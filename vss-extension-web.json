{
    "manifestVersion": 1,
    "id": "vsts-developer-tools-web",
    "version": "0.2.4",
    "publisher": "ms-devlabs",
    "name": "Extension Developer Tools for Web",
    "description": "Web tools to help you create and develop extensions and integrations to the Visual Studio Marketplace.",
    "icons": {
        "default": "tools-icon.png"
    },
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes": [
        "vso.code_write",
        "vso.build_execute",
        "vso.work_write",
        "vso.gallery_manage"
    ],
    "category": [
        "Code"
    ],
    "content": {
        "details": {
            "path": "readme.md"
        },
        "license": {
            "path": "license.txt"
        }
    },
    "links": {
        "repository": {
            "uri": "https://github.com/Microsoft/vsts-developer-tools"
        },
        "issues": {
            "uri": "https://github.com/Microsoft/vsts-developer-tools/issues"
        }
    },
    "files": [
        {
            "path": "Web",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "extend-hub-group",
            "type": "ms.vss-web.hub-group",
            "description": "Extend hub group",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Extend",
                "order": 100
            }
        },
        {
            "id": "extend-playground-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                ".extend-hub-group"
            ],
            "properties": {
                "name": "Playground",
                "uri": "Web/PlaygroundHub.html",
                "order": 20
            }
        },
        {
            "id": "extend-playground-run",
            "type": "ms.vss-web.control",
            "description": "Content for running playground in a dialog",
            "targets": [],
            "properties": {
                "uri": "Web/PlaygroundRun.html"
            }
        }
    ]
}