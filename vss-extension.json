{
  "manifestVersion": 1,
  "id": "vss-developer-tools",
  "version": "0.0.3",
  "publisher": "ms-devlabs",
  "name": "Extension Developer Tools",
  "description": "Tools to help you create, develop, and publish extensions and integrations to the Visual Studio Marketplace.",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": [
  ],
  "category": [
    "Code",
    "Build and Release"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "license.txt"
    }
  },
  "links": {
    "repository": {
      "uri": "https://github.com/Microsoft/vss-developer-tools"
    },
    "issues": {
      "uri": "https://github.com/Microsoft/vss-developer-tools/issues"
    }
  },
  "files": [
    {
      "path": "Web",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "extend-hub-group",
      "type": "ms.vss-web.hub-group",
      "description": "Extend hub group",
      "targets": [
        "ms.vss-web.project-hub-groups-collection"
      ],
      "properties": {
        "name": "Extend",
        "order": 100
      }
    },
    {
      "id": "extend-playground-hub",
      "type": "ms.vss-web.hub",
      "targets": [
        ".extend-hub-group"
      ],
      "properties": {
        "name": "Playground",
        "uri": "Web/PlaygroundHub.html",
        "order": 20
      }
    },
    {
      "id": "extend-playground-run",
      "type": "ms.vss-web.control",
      "description": "Content for running playground in a dialog",
      "targets": [],
      "properties": {
        "uri": "Web/PlaygroundRun.html"
      }
    }
  ]
}